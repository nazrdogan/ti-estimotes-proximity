function notify(e){Ti.App.iOS.scheduleLocalNotification({alertBody:e,alertAction:"OK",userInfo:{hello:"world"},date:new Date((new Date).getTime()+5)})}var TiBeacons=require("org.beuckman.tibeacons");TiBeacons.addEventListener("beaconRanges",function(e){for(var t in e.beacons){var i=e.beacons[t];switch(i.proximity){case"immediate":notify("Found "+i.major+"/"+i.minor+" in immediate proximity!");break;case"near":notify("Found "+i.major+"/"+i.minor+" in near proximity!")}}}),TiBeacons.startRangingForBeacons({uuid:"00000000-0000-0000-0000-000000000000",identifier:"TiBeacon Test"}),Ti.App.currentService.addEventListener("stop",function(){TiBeacons.stopRangingForBeacons()});